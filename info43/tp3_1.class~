public class tp3_1{
	
	public static int samoussa( int i,int j ) {
			int a
		if (i==1||j==1){
			a=1;
			 return a;
			 System.out.println(a);
		}
		else{
			a=samoussa(i,j-1)+samoussa(i-1,j);
			return a;
			 System.out.println(a);
					
		}
					
	}
	
	public static void main(String arg[]){
				int s=samoussa(3,2);
			
		}
	}
}